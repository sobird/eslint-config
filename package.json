{
  "name": "eslint-config-sobird",
  "type": "module",
  "version": "0.2.16",
  "packageManager": "pnpm@10.30.2",
  "description": "Sobird's ESLint config",
  "author": "sobird",
  "license": "MIT",
  "homepage": "https://github.com/sobird/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sobird/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/sobird/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config-sobird"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/cli.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "rules:types": "tsx scripts/rules",
    "preversion": "yarn lint:eslint",
    "version": "git add -A .",
    "postversion": "git push && git push --tags",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "tsx scripts/prepublish.ts"
  },
  "peerDependencies": {
    "eslint": "^9.10.0 || ^10.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
    "@eslint/compat": "^2.0.2",
    "@eslint/markdown": "^7.5.1",
    "@next/eslint-plugin-next": "^16.1.6",
    "@stylistic/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^8.56.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsonc": "^3.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.24.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.2",
    "eslint-plugin-vue": "^10.8.0",
    "eslint-plugin-yml": "^3.3.0",
    "globals": "^17.3.0",
    "jsonc-eslint-parser": "^2.4.2",
    "jsonc-parser": "^3.3.1",
    "ts-morph": "^27.0.2",
    "typescript-eslint": "^8.56.1",
    "vue-eslint-parser": "^10.4.0",
    "yaml-eslint-parser": "^2.0.0"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@commitlint/types": "^20.4.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^25.3.1",
    "camelcase": "^9.0.0",
    "eslint": "^10.0.2",
    "husky": "^9.1.7",
    "json-schema-to-typescript": "^15.0.4",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vue": "^3.5.29"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
