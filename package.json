{
  "name": "eslint-config-sobird",
  "type": "module",
  "version": "0.2.16",
  "description": "Sobird's ESLint config",
  "author": "sobird",
  "license": "MIT",
  "homepage": "https://github.com/sobird/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sobird/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/sobird/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config-sobird"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.16.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 10",
    "rules:types": "tsx scripts/rules",
    "preversion": "yarn lint:eslint",
    "version": "git add -A .",
    "postversion": "git push && git push --tags",
    "prepare": "husky",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "@eslint/compat": "^2.0.2",
    "@next/eslint-plugin-next": "^16.1.6",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@typescript-eslint/parser": "^8.54.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^17.3.0",
    "jsonc-eslint-parser": "^2.4.2",
    "jsonc-parser": "^3.3.1",
    "typescript-eslint": "^8.54.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^25.2.0",
    "camelcase": "^9.0.0",
    "husky": "^9.1.7",
    "json-schema-to-typescript": "^15.0.4",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vue": "^3.5.28"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
