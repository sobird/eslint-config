{
  "name": "eslint-config-sobird",
  "version": "0.2.16",
  "description": "Sobird's ESLint config",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "rollup -c -w --environment NODE_ENV:development",
    "lint:eslint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 10",
    "build": "rollup -c --environment NODE_ENV:production",
    "preversion": "yarn lint:eslint",
    "version": "git add -A .",
    "postversion": "git push && git push --tags",
    "prepare": "husky",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sobird/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "sobird",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sobird/eslint-config/issues"
  },
  "homepage": "https://github.com/sobird/eslint-config",
  "dependencies": {
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@stylistic/eslint-plugin": "^5.7.1",
    "eslint": "^9.39.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^14.0.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript-eslint": "^8.54.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "esbuild": "^0.19.5",
    "glob": "^10.3.10",
    "husky": "^9.1.7",
    "rollup": "^4.4.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "strip-ansi": "6.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
